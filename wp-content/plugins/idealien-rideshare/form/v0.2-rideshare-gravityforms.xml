<?xml version="1.0" encoding="UTF-8"?>
<forms version="1.6.11">
	<form labelPlacement="top_label" useCurrentUserAsAuthor="1" postAuthor="1" postCategory="1" postStatus="publish" postFormat="0">
		<title><![CDATA[Rideshare Primary Form v0.2.1]]></title>
		<description><![CDATA[Got a ride? Need a ride? Give a ride, get a ride! The best road trips are the ones where you make new friends. What better way to enjoy getting from A to B than getting to know someone and helping smog the planet just a little bit less at the same time.]]></description>
		<confirmation type="message">
			<message><![CDATA[Thanks for posting your rideshare! ]]></message>
		</confirmation>
		<button type="text">
			<text><![CDATA[Submit]]></text>
		</button>
		<fields>
			<field adminOnly="1" allowsPrepopulate="1" id="6" size="medium" type="post_title">
				<inputName><![CDATA[idealien_rideshare_postTitle]]></inputName>
				<label><![CDATA[Post Title]]></label>
				<saveAsCPT><![CDATA[idealien_rideshare]]></saveAsCPT>
			</field>
			<field adminOnly="1" allowsPrepopulate="1" id="32" isRequired="1" size="medium" type="post_custom_field" postCustomFieldName="idealien_rideshare_status" inputType="select" enableEnhancedUI="1">
				<defaultValue><![CDATA[Active]]></defaultValue>
				<cssClass><![CDATA[rideshareStatus]]></cssClass>
				<inputName><![CDATA[rideshare_status]]></inputName>
				<label><![CDATA[Status]]></label>
				<choices>
					<choice>
						<text><![CDATA[Dynamic Population]]></text>
					</choice>
				</choices>
			</field>
			<field allowsPrepopulate="1" id="5" isRequired="1" size="medium" type="post_custom_field" postCustomFieldName="idealien_rideshare_type" inputType="select" enableEnhancedUI="1">
				<cssClass><![CDATA[rideshareType]]></cssClass>
				<inputName><![CDATA[rideshare_type]]></inputName>
				<label><![CDATA[I want to]]></label>
				<choices>
					<choice>
						<text><![CDATA[Dynamic Population]]></text>
					</choice>
				</choices>
			</field>
			<field id="27" size="medium" type="radio">
				<cssClass><![CDATA[rideshareMode]]></cssClass>
				<inputName><![CDATA[idealien_rideshare_mode]]></inputName>
				<label><![CDATA[a ride to a]]></label>
				<choices>
					<choice isSelected="1">
						<text><![CDATA[City]]></text>
					</choice>
					<choice>
						<text><![CDATA[Event]]></text>
					</choice>
				</choices>
			</field>
			<field id="25" size="large" type="post_custom_field" postCustomFieldName="idealien_rideshare_destinationCity" inputType="text">
				<cssClass><![CDATA[rideshareCity floatField]]></cssClass>
				<label><![CDATA[to city]]></label>
				<conditionalLogic actionType="show" logicType="all">
					<rules>
						<rule fieldId="27" operator="is">
							<value><![CDATA[City]]></value>
						</rule>
					</rules>
				</conditionalLogic>
			</field>
			<field allowsPrepopulate="1" id="19" size="medium" type="post_custom_field" postCustomFieldName="idealien_rideshare_destinationStateProv" inputType="select" enableEnhancedUI="1">
				<cssClass><![CDATA[rideshareStateProv]]></cssClass>
				<inputName><![CDATA[idealien_rideshare_departureStateProv]]></inputName>
				<label><![CDATA[prov / state]]></label>
				<choices>
					<choice>
						<text><![CDATA[Dynamic Population]]></text>
					</choice>
				</choices>
				<conditionalLogic actionType="show" logicType="all">
					<rules>
						<rule fieldId="27" operator="is">
							<value><![CDATA[City]]></value>
						</rule>
					</rules>
				</conditionalLogic>
			</field>
			<field id="28" size="medium" type="post_custom_field" postCustomFieldName="idealien_rideshare_event" inputType="select" enableEnhancedUI="1">
				<cssClass><![CDATA[rideshare_event]]></cssClass>
				<label><![CDATA[to event]]></label>
				<choices>
					<choice>
						<text><![CDATA[Dynamic Population]]></text>
					</choice>
					<choice>
						<text><![CDATA[Add New Event]]></text>
					</choice>
				</choices>
				<conditionalLogic actionType="show" logicType="all">
					<rules>
						<rule fieldId="27" operator="is">
							<value><![CDATA[Event]]></value>
						</rule>
					</rules>
				</conditionalLogic>
			</field>
			<field id="29" size="medium" type="text">
				<cssClass><![CDATA[rideshare_newEvent]]></cssClass>
				<label><![CDATA[to a new event called]]></label>
				<conditionalLogic actionType="show" logicType="all">
					<rules>
						<rule fieldId="28" operator="is">
							<value><![CDATA[Add New Event]]></value>
						</rule>
					</rules>
				</conditionalLogic>
			</field>
			<field id="26" isRequired="1" size="large" type="post_custom_field" postCustomFieldName="idealien_rideshare_departureCity" inputType="text">
				<cssClass><![CDATA[rideshareCity floatField]]></cssClass>
				<label><![CDATA[from city]]></label>
			</field>
			<field allowsPrepopulate="1" id="22" isRequired="1" size="medium" type="post_custom_field" postCustomFieldName="idealien_rideshare_departureStateProv" inputType="select" enableEnhancedUI="1">
				<cssClass><![CDATA[rideshareStateProv]]></cssClass>
				<inputName><![CDATA[idealien_rideshare_departureStateProv]]></inputName>
				<label><![CDATA[prov / state]]></label>
				<choices>
					<choice>
						<text><![CDATA[Dynamic Population]]></text>
					</choice>
				</choices>
			</field>
			<field id="24" isRequired="1" size="medium" type="post_custom_field" postCustomFieldName="idealien_rideshare_spaces" inputType="select" enableEnhancedUI="1">
				<defaultValue><![CDATA[1]]></defaultValue>
				<cssClass><![CDATA[rideshareSpaces]]></cssClass>
				<label><![CDATA[for]]></label>
				<choices>
					<choice>
						<text><![CDATA[1]]></text>
					</choice>
					<choice>
						<text><![CDATA[2]]></text>
					</choice>
					<choice>
						<text><![CDATA[3]]></text>
					</choice>
					<choice>
						<text><![CDATA[4]]></text>
					</choice>
					<choice>
						<text><![CDATA[5+]]></text>
					</choice>
				</choices>
			</field>
			<field id="10" isRequired="1" size="medium" type="post_custom_field" postCustomFieldName="idealien_rideshare_departureDate" inputType="date" calendarIconType="calendar" dateType="datepicker">
				<defaultValue><![CDATA[{date_mdy}]]></defaultValue>
				<cssClass><![CDATA[rideshareDate]]></cssClass>
				<label><![CDATA[on]]></label>
			</field>
			<field adminOnly="1" id="13" size="medium" type="post_custom_field" postCustomFieldName="idealien_rideshare_username" inputType="text">
				<defaultValue><![CDATA[{user:user_login}]]></defaultValue>
				<inputName><![CDATA[user_login]]></inputName>
				<label><![CDATA[Username]]></label>
			</field>
			<field allowsPrepopulate="1" id="30" size="medium" type="post_custom_field" postCustomFieldName="idealien_rideshare_name" inputType="text">
				<cssClass><![CDATA[rideshare_name]]></cssClass>
				<inputName><![CDATA[idealien_rideshare_name]]></inputName>
				<label><![CDATA[Name]]></label>
			</field>
			<field allowsPrepopulate="1" id="31" size="medium" type="post_custom_field" postCustomFieldName="idealien_rideshare_email" inputType="email">
				<cssClass><![CDATA[rideshare_email]]></cssClass>
				<inputName><![CDATA[idealien_rideshare_email]]></inputName>
				<label><![CDATA[Email]]></label>
			</field>
			<field id="16" size="small" type="post_custom_field" postCustomFieldName="idealien_rideshare_addInfo" inputType="textarea">
				<description><![CDATA[Any other info you'd like the rideshare(r) or rideshare(e) to know about your ride request?]]></description>
				<cssClass><![CDATA[rideshareAddInfo]]></cssClass>
				<label><![CDATA[Additional Info]]></label>
			</field>
		</fields>
		<cssClass><![CDATA[idealien_rideshare_gf]]></cssClass>
		<notification>
			<subject><![CDATA[New submission from {form_title}]]></subject>
			<message><![CDATA[{all_fields}]]></message>
			<from><![CDATA[jamie@idealienstudios.com]]></from>
		</notification>
		<autoResponder>
			<from><![CDATA[jamie@idealienstudios.com]]></from>
		</autoResponder>
		<descriptionPlacement><![CDATA[below]]></descriptionPlacement>
	</form>
	<form labelPlacement="top_label" useCurrentUserAsAuthor="1">
		<title><![CDATA[Rideshare Comment Form v0.2.1]]></title>
		<confirmation type="message">
		</confirmation>
		<button type="text">
			<text><![CDATA[Submit]]></text>
		</button>
		<fields>
			<field id="15" size="medium" type="html" displayOnly="1">
				<content><![CDATA[<a href="#" onClick="rideshare_cancel();">Cancel</a>]]></content>
				<label><![CDATA[HTML Block]]></label>
			</field>
			<field adminOnly="1" allowsPrepopulate="1" id="11" size="medium" type="text">
				<cssClass><![CDATA[connect_mode]]></cssClass>
				<inputName><![CDATA[comment_mode]]></inputName>
				<label><![CDATA[Mode]]></label>
			</field>
			<field adminOnly="1" allowsPrepopulate="1" id="12" size="medium" type="text">
				<cssClass><![CDATA[connect_sender]]></cssClass>
				<inputName><![CDATA[connect_sender]]></inputName>
				<label><![CDATA[Sender User]]></label>
			</field>
			<field adminOnly="1" allowsPrepopulate="1" id="14" size="medium" type="text">
				<cssClass><![CDATA[connect_rideshare_id]]></cssClass>
				<inputName><![CDATA[connect_rideshare_id]]></inputName>
				<label><![CDATA[Rideshare ID]]></label>
			</field>
			<field adminOnly="1" allowsPrepopulate="1" id="13" size="medium" type="text">
				<description><![CDATA[WP User / Email]]></description>
				<cssClass><![CDATA[connect_receiver]]></cssClass>
				<inputName><![CDATA[connect_receiver]]></inputName>
				<label><![CDATA[Receiver]]></label>
			</field>
			<field id="16" size="medium" type="html" displayOnly="1">
				<content><![CDATA[Details placeholder]]></content>
				<cssClass><![CDATA[connect_details_display]]></cssClass>
				<label><![CDATA[Details]]></label>
			</field>
			<field allowsPrepopulate="1" id="6" size="medium" type="name" nameFormat="simple">
				<cssClass><![CDATA[connect_name]]></cssClass>
				<inputName><![CDATA[connect_name]]></inputName>
				<label><![CDATA[Name]]></label>
			</field>
			<field allowsPrepopulate="1" id="7" size="medium" type="email">
				<cssClass><![CDATA[connect_email]]></cssClass>
				<inputName><![CDATA[connect_email]]></inputName>
				<label><![CDATA[Email]]></label>
			</field>
			<field allowsPrepopulate="1" id="1" size="small" type="textarea">
				<description><![CDATA[All the important rideshare date and location details will be included along with your comments automatically.]]></description>
				<cssClass><![CDATA[connect_comments]]></cssClass>
				<label><![CDATA[Comments]]></label>
			</field>
		</fields>
		<descriptionPlacement><![CDATA[below]]></descriptionPlacement>
		<cssClass><![CDATA[idealien_rideshareComments]]></cssClass>
		<notification>
			<subject><![CDATA[New submission from {form_title}]]></subject>
			<message><![CDATA[{all_fields}]]></message>
			<from><![CDATA[{admin_email}]]></from>
		</notification>
		<autoResponder>
			<from><![CDATA[{admin_email}]]></from>
		</autoResponder>
	</form>
	<form labelPlacement="top_label" useCurrentUserAsAuthor="1">
		<title><![CDATA[Rideshare Delete Form v0.2.1]]></title>
		<confirmation type="message">
			<message><![CDATA[Successfully deleted.]]></message>
		</confirmation>
		<button type="text">
			<text><![CDATA[Yes]]></text>
		</button>
		<fields>
			<field id="15" size="medium" type="html" displayOnly="1">
				<content><![CDATA[<a href="#" onClick="rideshare_cancel();">No</a>]]></content>
				<label><![CDATA[HTML Block]]></label>
			</field>
			<field adminOnly="1" allowsPrepopulate="1" id="12" size="medium" type="text">
				<cssClass><![CDATA[delete_username]]></cssClass>
				<inputName><![CDATA[delete_username]]></inputName>
				<label><![CDATA[Username]]></label>
			</field>
			<field adminOnly="1" allowsPrepopulate="1" id="14" size="medium" type="text">
				<cssClass><![CDATA[delete_rideshare_id]]></cssClass>
				<inputName><![CDATA[delete_rideshare_id]]></inputName>
				<label><![CDATA[Rideshare ID]]></label>
			</field>
			<field id="16" size="medium" type="html" displayOnly="1">
				<cssClass><![CDATA[delete_details]]></cssClass>
				<label><![CDATA[Question]]></label>
			</field>
		</fields>
		<descriptionPlacement><![CDATA[below]]></descriptionPlacement>
		<cssClass><![CDATA[idealien_rideshareDelete]]></cssClass>
		<notification>
			<subject><![CDATA[New submission from {form_title}]]></subject>
			<message><![CDATA[{all_fields}]]></message>
			<from><![CDATA[{admin_email}]]></from>
		</notification>
		<autoResponder>
			<from><![CDATA[{admin_email}]]></from>
		</autoResponder>
	</form>
</forms>